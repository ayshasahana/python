    "#print a leap years with in a range\n",
    "a=int(input(\"enter the starting year:\"))\n",
    "b=int(input(\"enter the last year:\"))\n",
    "print(\"leap year from\",a,\"to\",b,\"is:\")\n",
    "for i in range (a,b):\n",
    " if(0==i%4)and(0!=i%100)or(0==i%400):\n",
    "        print(i)"
